program Main;
  var a, b : integer;
begin
  a := 1;
  b := 2;
  write('a > b => ');
  writeln(a > b);
  write('a < b => ');
  writeln(a < b);
  write('a = b => ');
  writeln(a = b);
  write('a >= b => ');
  writeln(a >= b);
  write('a <= b => ');
  writeln(a <= b);
  write('a <> b => ');
  writeln(a <> b);
end.